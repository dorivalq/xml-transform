			<xs:element name="readingTypeList" type="tns:ReadingTypeList" minOccurs="0"/>
			<xs:element name="eventCategory" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="retrieveRegisterReads" type="xs:boolean" minOccurs="0"/>
